{
  "name": "proyectointegrador",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://sebas-rom.github.io/ProyectoIntegrador",
  "type": "module",
  "scripts": {
    "deploy-docs": "npm run docs && gh-pages -d docs",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "docs": "typedoc --entryPointStrategy expand --tsconfig ./tsconfig.json --media ./src/uml/display --out docs && cp LICENSE.txt ./docs ",
    "firedeploy": "npm run build && firebase deploy --only hosting",
    "deploy-all": "npm run firedeploy && npm run deploy-docs"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.3",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@firebase/storage": "^0.12.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.5",
    "@paypal/react-paypal-js": "^8.2.0",
    "browser-image-compression": "^2.0.2",
    "date-fns": "^3.3.1",
    "diacritics": "^1.3.0",
    "firebase": "^10.7.2",
    "i18next": "^23.7.18",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^5.0.4",
    "react-element-to-jsx-string": "^15.0.0",
    "react-i18next": "^14.0.1",
    "react-number-format": "^5.3.3",
    "react-router-dom": "^6.21.3",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "typedoc": "^0.25.8",
    "typedoc-plugin-merge-modules": "^5.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
